<?xml version="1.0" ?>
<NetworkArbiter>
    <!--Settings for the WENDE C3 Arbiter     -->
    <!--IP Address of the server              -->
    <!--Port is the starting port for messages-->
    <ConnectionArbiter ip="192.168.1.70" port="4444" />
    <!--Settings for the WENDE C3 Processing Shared Memory Strings-->
    <SHM_FILE_CAMERA_STATUS FILE="SHM_C3_CAMERA_STATUS" MUTEX="SHM_C3_CAMERA_STATUS_MUTEX" EVENT_SERVER="SHM_C3_CAMERA_STATUS_EVENT1" EVENT_CLIENT="SHM_C3_CAMERA_STATUS_EVENT2" />
    <SHM_FILE_CAMERA_TRACK FILE="SHM_C3_CAMERA_TRACK" MUTEX="SHM_C3_CAMERA_TRACK_MUTEX" EVENT_SERVER="SHM_C3_CAMERA_TRACK_EVENT1" EVENT_CLIENT="SHM_C3_CAMERA_TRACK_EVENT2" />
    <SHM_FILE_CAMERA_IMAGE FILE="SHM_C3_CAMERA_IMAGE" MUTEX="SHM_C3_CAMERA_IMAGE_MUTEX" EVENT_SERVER="SHM_C3_CAMERA_IMAGE_EVENT1" EVENT_CLIENT="SHM_C3_CAMERA_IMAGE_EVENT2" />
    <SHM_FILE_LASER_STATUS FILE="SHM_C3_LASER_STATUS" MUTEX="SHM_C3_LASER_STATUS_MUTEX" EVENT_SERVER="SHM_C3_LASER_STATUS_EVENT1" EVENT_CLIENT="SHM_C3_LASER_STATUS_EVENT2" />
    <SHM_FILE_PROCESSING_STATUS FILE="SHM_C3_PROCESSING_STATUS" MUTEX="SHM_C3_PROCESSING_STATUS_MUTEX" EVENT_SERVER="SHM_C3_PROCESSING_STATUS_EVENT1" EVENT_CLIENT="SHM_C3_PROCESSING_STATUS_EVENT2" />
    <SHM_FILE_LASER_TRACK FILE="SHM_C3_LASER_POINTING" MUTEX="SHM_C3_LASER_POINTING_MUTEX" EVENT_SERVER="SHM_C3_LASER_POINTING_EVENT1" EVENT_CLIENT="SHM_C3_LASER_POINTING_EVENT2" />
    <!--Debug Configuration items-->
    <!--Settings for the WENDE C3 2-State Kalman Filter-->
    <KALMAN_FILTER PROCESS_NOISE="9" />
    <!--Settings for the WENDE ENVIROMENT SETTINGS-->
    <ENVIROMENT PLAYING_FIELD_RADIUS="0.7" FAILURE_LINE_RADIUS="2" />
    <!--Debug Configuration items-->
    <DEBUG_PANNEL ENABLE="0" />
    <!--Settings for the WENDE C3 Driver (DEBUG)     -->
    <!--IP Address of the server              -->
    <!--Port is the starting port for messages-->
    <ConnectionDriver ip="192.168.1.70" port="4447" />
    <StartupPaths Arbiter="NetworkArbiter.exe" Processing="C3ProcessingApp.exe" Driver="ServerSocket.exe" />
</NetworkArbiter>
